    .data
const_1:    .num    1
out_1:      .num    1
in_1:       .num    1 ;пусть передана паскаль строка
sym_count:  .num    0
mask:       .num    255
mult:       .num    256 ;константа для корректного парсинга чисел
byt_count:  .num    1
total_b_co: .num    16777216
    .text
_start:
len_fetch:
    input       in_1
    mul         byt_count
    add         sym_count ;запись 4-х байного числа в переменную sym_count
    write       sym_count
    read        byt_count
    sub         total_b_co
    beq         loop

    read        byt_count
    mul         mult
    write       byt_count
    jump        len_fetch
loop:
    input       in_1
    output      out_1
    read        sym_count
    sub         const_1
    beq         end ;проверка на конец
    write       sym_count
    jump        loop
end:
    halt
